# Food Profit Flow - Environment Variables
# Copy this file to .env and fill in your values

# PostgreSQL Database URL (required)
# Railway provides this automatically when you add a PostgreSQL service
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Session secret - use a long random string in production
# Generate with: openssl rand -hex 32
SESSION_SECRET=change-this-to-a-long-random-secret-in-production

# Node environment
NODE_ENV=production

# Port (Railway sets this automatically - do not change)
PORT=8080
